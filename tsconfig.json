{
  "compilerOptions": {
    "target": "es5",
		"module": "esnext",
    "lib": [
			"dom",
			"dom.iterable",
			"ESNext"
		],
		"moduleResolution": "node",
    "allowJs": true,
		"jsx": "react",
		"sourceMap": true,
		"outDir": "./build",
    "strict": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
  }
}
