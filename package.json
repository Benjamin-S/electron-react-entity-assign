{
	"name": "multi-entity-management-tool",
	"version": "0.1.4",
	"private": true,
	"main": "public/electron.js",
	"repository": "Benjamin-S/electron-react-entity-assign",
	"author": {
		"name": "Benjamin Symons",
		"email": "mail@bensymons.com",
		"url": "https://bensymons.com"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"bootstrap": "^4.3.1",
		"cors": "^2.8.5",
		"electron-better-ipc": "^1.1.1",
		"electron-is-dev": "^1.1.0",
		"electron-timber": "^0.5.1",
		"electron-util": "^0.14.2",
		"express": "^4.17.1",
		"mssql": "^5.1.0",
		"react": "^16.11.0",
		"react-bootstrap": "^1.0.0-beta.14",
		"react-bootstrap-typeahead": "^3.4.7",
		"react-dom": "^16.11.0",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.2.0",
		"react-transition-group": "^4.3.0"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"test": "yarn lint",
		"start": "react-scripts start",
		"release": "np",
		"electron-pack": "electron-builder --c.extraMetadata.main=build/electron.js",
		"build": "react-scripts build",
		"electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
		"electron-built": "electron . Built",
		"dist": "yarn build && yarn electron-pack"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^5.0.0",
		"cross-env": "^6.0.3",
		"electron": "^10.1.3",
		"electron-builder": "^22.1.0",
		"electron-debug": "^3.1.0",
		"eslint-config-xo": "^0.29.1",
		"eslint-config-xo-react": "^0.23.0",
		"eslint-plugin-react": "^7.21.3",
		"eslint-plugin-react-hooks": "^4.1.2",
		"node-sass": "^4.13.0",
		"np": "^6.5.0",
		"wait-on": "^3.3.0",
		"xo": "^0.30.0"
	},
	"xo": {
		"extends": "xo-react",
		"envs": [
			"node",
			"browser"
		],
		"rules": {
			"import/no-unassigned-import": [
				"error",
				{
					"allow": [
						"**//*.css",
						"**//*.scss"
					]
				}
			]
		},
		"parser": "babel-eslint"
	},
	"np": {
		"publish": false,
		"releaseDraft": false
	},
	"build": {
		"appId": "com.bensymons.mem-tool",
		"productName": "Entity Assignment",
		"files": [
			"build/**/*",
			"node_modules/**/*",
			"./src/*.js",
			"./src/*.png"
		],
		"directories": {
			"buildResources": "assets"
		},
		"win": {
			"target": "portable",
			"icon": "build/icon.png"
		}
	},
	"homepage": "."
}
